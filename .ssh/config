IdentitiesOnly=yes
Host arch-server.lan
  IdentityFile ~/.ssh/zhifan_archserver
Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur
Host github.com
  IdentityFile ~/.ssh/personal_gh
Host *
  ServerAliveInterval 60
  #  RequestTTY yes
Host github.com
  User git
  Hostname github.com
  PreferredAuthentications publickey
  IdentityFile /home/zhifan/.ssh/personal_gh
Host bitbucket.org
    User git
    Hostname bitbucket.org
    PreferredAuthentications publickey
    IdentityFile /home/zhifan/.ssh/bitbucket_hrm
Host gitlab.com
    User git
    Hostname gitlab.com
    PreferredAuthentications publickey
    IdentityFile /home/zhifan/.ssh/personal_gl
Host simpolab.com
    User zhifan
    Hostname simpolab.com
    IdentityFile ~/.ssh/simpolab
    PreferredAuthentications publickey
